{
  "name": "vue-latest",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build --mode production",
    "type-check": "vue-tsc --build",
    "lint": "eslint . --fix",
    "format": "prettier --write src/",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix --ignore-pattern *.d.ts --ignore-pattern src/components/ui/**/* --no-warn-ignored",
      "prettier --write src/"
    ]
  },
  "dependencies": {
    "@iconify/vue": "^5.0.0",
    "@tailwindcss/vite": "^4.1.8",
    "@tanstack/vue-table": "^8.21.3",
    "@types/crypto-js": "^4.2.2",
    "@vee-validate/zod": "^4.15.1",
    "@vueuse/core": "^13.9.0",
    "apexcharts": "^4.7.0",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "crypto-js": "^4.2.0",
    "lucide-vue-next": "^0.513.0",
    "pinia": "^3.0.1",
    "pinia-plugin-persistedstate": "^4.3.0",
    "reka-ui": "^2.6.0",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.4",
    "unplugin-vue-components": "^28.7.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0",
    "vue-sonner": "^2.0.0",
    "vue3-apexcharts": "^1.8.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.1",
    "@types/node": "^22.15.30",
    "@vitejs/plugin-vue": "^5.2.4",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.5.0",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.28.0",
    "eslint-plugin-vue": "~10.2.0",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "3.5.3",
    "typescript": "~5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-vue-devtools": "^7.7.2",
    "vue-tsc": "^2.2.8"
  }
}
